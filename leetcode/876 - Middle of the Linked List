/**
 * Definition for singly-linked list.
 * public class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) { val = x; }
 * }
 */
class Solution {
    public ListNode middleNode(ListNode head) {
        int num = 0;

        ListNode current  = head;
        while(current!= null)
        {
            current = current.next;
            ++num;
        }
        current  = head;
         for(int i = 0; i< num/2;i++)
        {
            current = current.next;
        }
        return current;
    }
}
